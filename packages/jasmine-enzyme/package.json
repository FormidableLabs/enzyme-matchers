{
  "name": "jasmine-enzyme",
  "version": "1.3.0-beta2",
  "description": "Jasmine assertions for enzyme",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "lint": "eslint ./src",
    "build": "babel src --out-dir lib --ignore tests"
  },
  "files": [
    "src",
    "lib"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/blainekasten/jasmine-enzyme.git"
  },
  "keywords": [
    "javascript",
    "shallow rendering",
    "shallowRender",
    "test",
    "reactjs",
    "react",
    "flux",
    "testing",
    "test utils",
    "assertion helpers",
    "tdd",
    "jasmine",
    "jest",
    "enzyme"
  ],
  "author": "Blaine Kasten<blainekasten@gmail.com>",
  "license": "MIT",
  "peerDependencies": {
    "enzyme": "1.x || 2.x"
  },
  "dependencies": {
    "enzyme-matchers": "1.3.0-beta"
  },
  "jest": {
    "testPathDirs": [
      "<rootDir>/src"
    ],
    "setupFiles": [
      "../../scripts/jest-setup.js"
    ],
    "preprocessorIgnorePatterns": [
      "node_modules/(?!enzyme-matchers)"
    ],
    "unmockedModulePathPatterns": [
      "enzyme",
      "react"
    ],
    "verbose": true
  }
}
